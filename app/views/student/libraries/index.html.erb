<div class="bg-white"> 
  <div class="row">
    <% @libraries.each do |library| %>
      <div class="col-4">
        <%= render partial: 'library', locals: {library: library} %>
      </div>
    <% end%>
  </div>
  <div class="row">
    <div class="col-6 text-center">
      <%= will_paginate @library,renderer: BootstrapPagination::Rails %>
    </div>
  </div>
</div>